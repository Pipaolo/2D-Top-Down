            <ui:Button text="Scadoosh" display-tooltip-when-elided="true" name="ScadooshBtn" class="button-style" style="flex-grow: 1; height: 120px; border-top-left-radius: 12px; border-bottom-left-radius: 12px; border-top-right-radius: 12px; border-bottom-right-radius: 12px; font-size: 24px;" />
            <ui:Button text="Sniperist" display-tooltip-when-elided="true" name="SniperistBtn" class="button-style" style="flex-grow: 1; height: 120px; border-top-left-radius: 12px; border-bottom-left-radius: 12px; border-top-right-radius: 12px; border-bottom-right-radius: 12px; font-size: 24px;" />
