        <ui:Label text="Select Your Class" display-tooltip-when-elided="true" name="Title" style="-unity-font-style: bold; font-size: 24px; color: rgb(255, 255, 255); -unity-text-align: upper-center; margin-left: 12px; margin-right: 12px; margin-top: 12px; margin-bottom: 12px;" />
        <ui:VisualElement name="ClassList" style="flex-direction: row; flex-grow: initial; flex-basis: 100%; justify-content: center; align-items: center; width: auto; flex-wrap: nowrap; margin-left: 12px; margin-right: 12px; margin-top: 12px; margin-bottom: 12px;">
